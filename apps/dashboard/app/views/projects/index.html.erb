<div class='page-header text-center'>
  <h1>Jobs App</h1>
  <%= link_to 'New Project', new_project_path, class: 'btn btn-info', 
  title: I18n.t('dashboard.jobs_project_create_new_project_directory') %>
</div>
<br>
<div class='card'>
  <h3 class='card-header'>Projects</h3>
  <div class='card-body'>
    <table class='table table-borderless'>
      <thead>
        <tr>
          <th>Project Name</th>
          <th>Actions</th>
        <tr>
      </thead>
      <tbody>
      <% @projects.each do |project| %>
        <%= project.inspect %><hr />
      <% end %>
      </tbody>
    </table>
  </div>
</div>
